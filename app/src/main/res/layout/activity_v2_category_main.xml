                        android:id="@+id/namaitem"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_marginLeft="10dp"
                        android:text="View : "
                        android:textColor="@color/colorPrimary"
                        android:textSize="13dp"
                        app:textFontPath="fonts/popinregural.otf"


                        />

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageView
                            android:id="@+id/gridgray"
                            android:layout_width="40dp"
                            android:layout_height="30dp"
                            android:src="@drawable/ic_gridgray"
                            android:visibility="gone" />

                        <ImageView
                            android:id="@+id/gridprimary"
                            android:layout_width="40dp"
                            android:layout_height="30dp"
                            android:src="@drawable/ic_gridprimary" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp">

                        <ImageView
                            android:id="@+id/listgray"
                            android:layout_width="40dp"
                            android:layout_height="30dp"
                            android:src="@drawable/ic_listgray" />

                        <ImageView
                            android:id="@+id/listprimary"
                            android:layout_width="40dp"
                            android:layout_height="30dp"
                            android:src="@drawable/ic_listprimary"
                            android:visibility="gone" />
                    </RelativeLayout>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="80dp">

